{
  "name": "sonarqube-api-client",
  "version": "0.0.1",
  "description": "Community SonarQube Web API client for JavaScript!",
  "private": true,
  "type": "module",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "lint": "eslint --cache --fix .",
    "lint:check": "eslint --cache .",
    "format": "prettier --cache --write --ignore-unknown .",
    "format:check": "prettier --cache --check --ignore-unknown .",
    "test": "jest --coverage",
    "release": "npx semantic-release",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.2",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "p-limit": "^6.2.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "rollup-plugin-copy": "^3.5.0",
    "semantic-release": "^24.2.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "keywords": [
    "sonar",
    "sonarqube",
    "api",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/titan-ron/sonarqube-api-client/issues"
  },
  "homepage": "https://github.com/titan-ron/sonarqube-api-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/titan-ron/sonarqube-api-client.git"
  }
}
